
@{
    ViewBag.Title = "List Teams";
}

<h2>Teams datas</h2>

<form class="form-horizontal">
    <div class="form-group">
        <label for="" class="col-sm-2 control-label">Name</label>
        <div class="col-sm-8">
            <input type="" class="form-control" id="teamName" placeholder="Name">
        </div>
    </div>
    <div class="form-group">
        <label for="" class="col-sm-2 control-label">Category</label>
        <div class="col-sm-8">
            <input type="" class="form-control" id="teamCategory" placeholder="Category">
        </div>
    </div>

    <br/>

    <input id="create" type="button" value="Save" class="button">
    <input id="clean" type="button" value="Cancel" class="button">
    <input id="update" type="button" value="Update">
</form>

<br/>

<hr/>

<h2>Table Teams</h2>

<table id="tableTeams" class="table table-striped">
    <thead>
        <tr>
            <th>Team Id</th>
            <th>Team Name</th>
            <th>Category</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
    </thead>

    <tbody>
      
    </tbody>
</table>